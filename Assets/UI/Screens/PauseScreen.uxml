            <ui:Button text="Main menu&#10;" display-tooltip-when-elided="true" name="MainMenu" class="menu-item" style="border-left-width: 0; border-right-width: 0; border-top-width: 0; border-bottom-width: 0; padding-bottom: 30px;" />
